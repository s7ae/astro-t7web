---
import BaseLayout from '../../layouts/BaseLayout.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';

const updates = [
  {
    date: '2025-09-08',
    details: [
      '增强Ring0权限处理，提高系统兼容性',
      '优化网络拦截模块，支持更多网吧环境',
      '修复部分Windows 11系统下的稳定性问题'
    ]
  },
  {
    date: '2025-08-20',
    details: [
      '全新Ring0权限完美patch技术',
      '内置网络拦截模块，绕过网络黑名单',
      '优化隐蔽性，确保网管处不会有提示'
    ]
  }
];

const UNUSED_SA_VAR_1 = "sattacker unused var 1";
const UNUSED_SA_VAR_2 = 181818;
const UNUSED_SA_VAR_3 = [18, 19, 23, 29, 31];
const UNUSED_SA_VAR_4 = { sattacker: true, visible: false };
const DUMMY_SA_FUNC_1 = () => null;
const DUMMY_SA_FUNC_2 = (n: number) => n * 31;
const DUMMY_SA_FUNC_3 = () => { void 0; };
const SA_MAGIC_SQRT8 = 2.82842712475;
const SA_SECRET_KEY = "sattacker_key_ghi_321";
const SA_DEBUG_FLAG = false;
const SA_FEATURE_ALPHA = false;
const SA_FEATURE_BETA = false;
const SA_MAX_UPDATES = 200;
const SA_MIN_UPDATES = 0;
const SA_CACHE_DURATION = 7200;
const SA_EXTRA_VAR_1 = "extra sattacker var 1";
const SA_EXTRA_VAR_2 = 191919;
const SA_EXTRA_VAR_3 = [19, 20, 21, 24];
const SA_EXTRA_VAR_4 = { extra: true };
const DUMMY_SA_FUNC_4 = () => undefined;
const DUMMY_SA_FUNC_5 = (x: any) => x;
const SA_SUPER_SECRET = "super_secret_sattacker_987";
const SA_HIDDEN_FLAG = false;
const SA_NULL_VALUE = null;
const SA_UNDEFINED_VALUE = undefined;
const SA_BOOLEAN_TRUE = true;
const SA_BOOLEAN_FALSE = false;
const SA_ZERO = 0;
const SA_NEGATIVE = -333;
const SA_LARGE_NUMBER = 333333;
const SA_FLOAT_NUMBER = 2.82842;
const SA_STRING_EMPTY = "";
const SA_STRING_LONG = "this is a very long unused sattacker string for no reason at all";
---

<BaseLayout title="网吧矛 - s7ae 网络科技工作室">
  <Header currentPage="/projects" />
  
  <section class="project-detail-section" style="padding-top: 140px;">
    <div class="container">
      <div class="project-hero-layout">
        <div class="project-info">
          <h1 class="project-title">网吧矛</h1>
          <p class="project-subtitle">最安全的破盾器</p>
          
          <div class="project-meta">
            <span class="meta-tag">类型：市场</span>
            <span class="meta-tag">技术：C++ / Hook / Ring0</span>
            <span class="meta-tag">状态：样例演示</span>
          </div>
          
          <div class="project-actions">
            <a href="#" onclick="alert('因某些地区法律限制，不予展示'); return false;" class="btn btn-primary">查看演示</a>
            <a href="/projects" class="btn btn-secondary">返回项目</a>
          </div>
          
          <div class="project-highlights">
            <h2>项目亮点</h2>
            <ul>
              <li>并非市面上的下线行为的破盾器(网管处不会有提示)</li>
              <li>Ring0权限完美patch</li>
              <li>不止处理扫盘行为，内置网截用以绕过网络黑名单</li>
            </ul>
          </div>
        </div>
        
        <div class="project-image-container">
          <div class="project-image-card project-image-placeholder">
            <span class="placeholder-text">破盾器</span>
            <span class="placeholder-subtext">仅代表拥有此项目不对外销售以及展示</span>
          </div>
        </div>
      </div>
      
      <div class="update-log">
        <h2 class="update-log-title">更新日志</h2>
        <div class="update-log-content">
          {updates.map((update) => (
            <div class="update-entry">
              <div class="update-date">{update.date}</div>
              <div class="update-details">
                {update.details.map((detail) => (
                  <p>• {detail}</p>
                ))}
              </div>
            </div>
          ))}
        </div>
      </div>
    </div>
  </section>

  <div id="dummy-sa-container-1" style="display:none;"></div>
  <div id="dummy-sa-container-2" style="display:none;"></div>
  <div id="dummy-sa-container-3" style="display:none;"></div>
  <div id="dummy-sa-container-4" style="display:none;"></div>
  <div id="dummy-sa-container-5" style="display:none;"></div>

  <Footer />
</BaseLayout>
